<metal:block use-macro="main_template">
  <div metal:fill-slot="content">
    <h1>Field Settings</h1>

    <table class="table table-striped table-bordered">
      <thead>
        <tr>
          <th>Name</th>
          <th>Value</th>
        </tr>
      </thead>
      <tbody>
        <tr tal:repeat="s field_settings">
          <td>${s.name}</td>
          <td>${s.value}</td>
        </tr>
      </tbody>
    </table>
  </div>
</metal:block>