<metal:block use-macro="main_template">
  <div metal:fill-slot="content">
    <h1>Stage</h1>
    <h2>${stage.name}</h2>

    <table class="table table-striped table-bordered">
      <tbody>
        <tr>
          <th>Number</th>
          <td>${stage.number}</td>
        </tr>
        <tr>
          <th>Duration</th>
          <td>${stage.duration}</td>
        </tr>
      </tbody>
    </table>
  </div>
</metal:block>