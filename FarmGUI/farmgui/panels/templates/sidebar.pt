<div class="col-sm-2">
    <ul id="config-sidebar" class="nav nav-stacked affix">
        <li tal:repeat="item configuration_items" tal:attributes="class 'active' if item.active else ''">
            <a href="${item.url}">${item.name}</a>
        </li>
    </ul>
    <ul id="plot-sidebar" class="nav menu">
        <li class="deeper parent" tal:repeat="comp components">
            <input type="checkbox" name="${comp.name}" id="${comp.name+'_checkbox'}" data="${comp.id}"/>
            <label for="${comp.name+'_checkbox'}">${comp.name}</label>
        </li>
    </ul>
</div>
      